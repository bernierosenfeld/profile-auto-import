{
  "name": "Profile Auto-Import",
  "version": "1.0",
  "description": "Import a candidate profile at the press of a button!",
  "permissions": [
    "downloads",
    "tabs",
    "declarativeContent",
    "storage"
  ],
  "options_page": "options.html",
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "commands": {
    "_execute_page_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Q",
        "windows": "Ctrl+Shift+Q",
        "mac": "MacCtrl+Shift+Q"
      },
      "description": "Opens popup.html"
    }
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://www.dice.com/employer/talent/profile/*"],
      "js": ["jquery-3.5.1.min.js", "utils.js", "diceImport.js"]
    },
    {
      "matches": ["https://hiring.monster.com/searchmonster/job/*"],
      "js": ["jquery-3.5.1.min.js", "utils.js", "monsterImport.js"]
    }
  ],
  "manifest_version": 2
}